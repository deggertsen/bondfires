{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "android:start": "expo start --android",
    "ios:start": "expo start --ios",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "web": "expo start --web",
    "build": "echo 'Use EAS Build for production builds'",
    "build:android:dev": "eas build --platform android --profile development",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:dev:sim": "eas build --platform ios --profile development-simulator",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:ios:prod": "eas build --platform ios --profile production",
    "build:android:prod": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "release:ios": "yarn build:ios:prod && yarn submit:ios",
    "release:android": "yarn build:android:prod && yarn submit:android",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bondfires/app": "workspace:*",
    "@bondfires/config": "workspace:*",
    "@bondfires/ui": "workspace:*",
    "@convex-dev/auth": "^0.0.90",
    "@expo/vector-icons": "^15.0.3",
    "@legendapp/state": "^3.0.0-beta.27",
    "@react-navigation/native": "^7.1.8",
    "@tamagui/animations-react-native": "^1.144.0",
    "@tamagui/babel-plugin": "^1.144.0",
    "@tamagui/config": "^1.144.0",
    "@tamagui/font-inter": "^1.144.0",
    "@tamagui/lucide-icons": "^1.144.0",
    "@tamagui/metro-plugin": "^1.144.1",
    "@tamagui/shorthands": "^1.144.0",
    "@tamagui/themes": "^1.144.0",
    "convex": "^1.31.3",
    "expo": "~54.0.31",
    "expo-av": "^16.0.8",
    "expo-background-fetch": "~14.0.9",
    "expo-camera": "^17.0.10",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "^6.0.20",
    "expo-device": "^8.0.10",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.10",
    "expo-image": "^3.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "^0.32.16",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-task-manager": "~14.0.9",
    "expo-video": "~3.0.15",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-compressor": "^1.16.0",
    "react-native-mmkv": "^4.1.1",
    "react-native-nitro-modules": "^0.32.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "~15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tamagui": "^1.144.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "babel-preset-expo": "~54.0.9",
    "expo-build-properties": "^1.0.10",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
